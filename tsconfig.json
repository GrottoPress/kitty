{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "module": "es2020",
    "target": "es2020",
    "lib": ["DOM"],
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
  }
}
