{
  "name": "@grottopress/kitty",
  "type": "module",
  "version": "0.6.0",
  "description": "Kitty is a collection of utilities for SvelteKit",
  "license": "MIT",
  "author": {
    "name": "GrottoPress",
    "email": "info@grottopress.com",
    "url": "https://www.grottopress.com"
  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "package": "svelte-package",
    "preview": "vite preview --port 3000",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest tests/unit",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:e2e": "playwright test tests/e2e",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@sveltejs/package": "^1.0.1",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.15.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "isomorphic-fetch": "^3.0.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.2",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.5.10",
    "ts-jest": "^29.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrottoPress/kitty.git"
  },
  "homepage": "https://github.com/GrottoPress/kitty#README",
  "keywords": [
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  }
}
